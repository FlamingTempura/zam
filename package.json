{
  "author": "FlamingTempura",
  "description": "Lightweight data binding",
  "homepage": "https://github.com/FlamingTempura/zam",
  "main": "zam.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "name": "zam",
  "scripts": {
    "test": "find test -type f -not -iname test-utils.js -exec tap -j6 {} +",
