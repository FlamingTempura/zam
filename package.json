{
  "author": "FlamingTempura",
  "description": "Lightweight data binding",
  "homepage": "https://github.com/FlamingTempura/zam",
  "main": "zam.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "name": "zam",
  "scripts": {
    "test": "find test -type f -not -iname test-utils.js -exec tap -j6 {} +",
    "cover": "find test -type f -not -iname test-utils.js -exec tap -j6 --cov --coverage-report=lcov {} +",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "version": "0.6.2",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "jsdom": "^11.3.0",
    "moment": "^2.19.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-esformatter": "^0.4.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-pegjs": "^2.1.3",
    "rollup-plugin-strip": "^1.1.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-watch": "^4.3.1",
    "tap": "^10.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlamingTempura/zam.git"
  },
  "dependencies": {
    "babel-tap": "^5.0.0"
  }
}
