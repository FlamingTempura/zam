    "jsdom": "^11.0.0",
    "moment": "^2.18.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-esformatter": "^0.3.0",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-pegjs": "^2.1.3",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "tap": "^10.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlamingTempura/zam.git"
